{
  "name": "@kwonkunkun/convenient-script",
  "version": "1.0.0",
  "description": "Kwonkunkun's convenient script",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "release-it": "^15.10.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "release": "release-it",
    "run-banner": "chmod +x ./scripts/run-banner.sh && ./scripts/run-banner.sh",
    "run-done-banner": "chmod +x ./scripts/run-done-banner.sh && ./scripts/run-done-banner.sh",
    "set-all": "chmod +x ./set-all.sh && ./set-all.sh",
    "set-husky": "chmod +x ./scripts/set-husky.sh && ./scripts/set-husky.sh",
    "set-commitlint": "chmod +x ./scripts/set-commitlint.sh && ./scripts/set-commitlint.sh",
    "set-commitizen": "chmod +x./scripts/set-commitizen.sh && ./scripts/set-commitizen.sh",
    "set-release-it": "chmod +x./scripts/set-release-it.sh && ./scripts/set-release-it.sh"
  },
  "bin": {
    "set-kwon-env": "../bin/run-command.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kwonkunkun/my-convenient-shell-scripts.git"
  },
  "keywords": [
    "shell",
    "script"
  ],
  "author": "Kwonkunkun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kwonkunkun/my-convenient-shell-scripts/issues"
  },
  "homepage": "https://github.com/Kwonkunkun/my-convenient-shell-scripts#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "commander": "^10.0.1",
    "inquirer": "^9.1.5"
  }
}
